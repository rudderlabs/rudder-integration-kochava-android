<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rudderstack.android.sample.kotlin.MainActivity">

    <Button
        android:id="@+id/identify"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Identify"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/reset"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="reset"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/identify" />

    <Button
        android:id="@+id/orderCompletedWithMultipleProductsAsList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Order Completed With Multiple Products As List"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/reset" />

    <Button
        android:id="@+id/orderCompletedWithMultipleProductsAsJSON"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Order Completed With Multiple Products As JSON"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/orderCompletedWithMultipleProductsAsList" />

    <Button
        android:id="@+id/orderCompletedWithSingleProductAsJSON"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Order Completed With Single Product As JSON"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/orderCompletedWithMultipleProductsAsJSON" />

    <Button
        android:id="@+id/checkoutStarted"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Checkout Started"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/orderCompletedWithSingleProductAsJSON" />

    <Button
        android:id="@+id/productAdded"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Product Added"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/checkoutStarted" />

    <Button
        android:id="@+id/productAddedToWishlist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Product Added To Wishlist"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/productAdded" />

    <Button
        android:id="@+id/productReviewed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Product Reviewed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/productAddedToWishlist" />

    <Button
        android:id="@+id/productSearched"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Product Searched"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/productReviewed" />

    <Button
        android:id="@+id/customTrackWithProperties"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Custom Track With Properties"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/productSearched" />

    <Button
        android:id="@+id/customTrackWithoutProperties"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Custom Track Without Properties"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/customTrackWithProperties" />

    <Button
        android:id="@+id/screenWithProperties"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Screen With Properties"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/customTrackWithoutProperties" />

    <Button
        android:id="@+id/screenWithoutProperties"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Screen Without Properties"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/screenWithProperties" />
</androidx.constraintlayout.widget.ConstraintLayout>
